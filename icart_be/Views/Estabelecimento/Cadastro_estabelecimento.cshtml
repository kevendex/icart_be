@model icart_be.Models.Usuario
@{
    ViewData["Title"] = "Cadastro estabelecimento";
}

<head>
    <link rel="stylesheet" href="~/css/cadastro-perfil.css">
    <link rel="stylesheet" href="~/css/index.css">
    <style>
        footer {
            position: fixed;
            bottom: 0;
            left: 0;
        }
    </style>
</head>
<br>
<br>
<br>
<br>
<br>
<br>
<br>
<div id="container-cadastro">
    <br><br><br>
    <h1 id="titulo-principal">Informações do estabelecimento</h1>
    <br><br><br><br><br>
    <form action="post" id="form-cadastro">
        <div id="container-linhas-lacunas">
            <div id="container-lacuna"><input type="text" name="nome_empresarial" id="lacuna" placeholder="Nome Empresarial:" minlength="1" maxlength="45"></div>
            <div id="container-lacuna"><input type="text" name="nome_fantasia" id="lacuna" placeholder="Nome Fantasia:" minlength="1" maxlength="45"></div>
            <div id="container-lacuna"><input type="text" name="cnpj" id="lacuna" placeholder="CNPJ:" minlength="14" maxlength="14"></div>
            <div id="container-lacuna"><input type="text" name="cep" id="lacuna" placeholder="CEP:" minlength="8" maxlength="8"></div>
        </div>
        <br>
        <br>
        <br>
        <div id="container-linhas-lacunas">
            <div id="container-lacuna"><input type="number" name="num_endereco" id="lacuna" placeholder="Número:" minlength="1" maxlength="10"></div>
            <div id="container-lacuna"><input type="text" name="tamanho" id="lacuna" placeholder="Tamanho Estabelecimento:" minlength="8" maxlength="8"></div>
            <div id="container-lacuna"><input type="text" name="bairro" id="lacuna" placeholder="Bairro:" minlength="1" maxlength="60"></div>
            <div id="container-lacuna"><input type="text" name="municipio" id="lacuna" placeholder="Municipio:" minlength="1" maxlength="35"></div>
        </div>
        <br>
        <br>
        <br>
        <div id="container-linhas-lacunas">
            <div id="container-lacuna"><input type="text" name="uf" id="lacuna" placeholder="UF:" minlength="2" maxlength="2"></div>
            <div id="container-lacuna"><input type="text" name="logradouro" id="lacuna" placeholder="Logradouro:" minlength="1" maxlength="60"></div>
            <div id="container-lacuna"><input type="text" name="email" id="lacuna" placeholder="E-mail:" minlength="8" maxlength="35"></div>
            <div id="container-lacuna"><input type="text" name="telefone" id="lacuna" placeholder="Telefone:" minlength="11" maxlength="11"></div>
        </div>
        <br><br><br>
        <div id="container-linhas-lacunas">
            <div id="container-lacuna"><input type="text" name="complemento" id="lacuna" placeholder="Complemento:" minlength="0" maxlength="45"></div>
            <div id="container-lacuna"><input type="password" name="senha" id="lacuna" placeholder="Senha:" minlength="8" maxlength="16"></div>
            <div id="container-lacuna"><input type="number" name="codigo" id="lacuna" placeholder="Cod. do Estabel:" minlength="1" maxlength="6"></div>
            <div id="container-lacuna"></div>
        </div>
        <br><br><br><br><br><br><br><br><br>
        <div id="container-button-cadastrar">
            <input type="submit" value="Cadastrar" id="bto-cadastrar">
        </div>
    </form>
</div>