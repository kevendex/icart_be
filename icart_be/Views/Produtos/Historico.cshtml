@model List<icart_be.Models.Venda>
@{
    ViewData["Title"] = "Histórico de vendas";
}

<head>
    <link rel="stylesheet" href="css/historico.css">
    <link rel="stylesheet" href="css/index.css">
    <style>
        footer {
            position: fixed;
            bottom: 0;
            left: 0;
        }
    </style>
</head>
<br>
<br>
<div id="box-historico-vendas">
    <h1 class="titulo-grande">
        Últimas vendas
    </h1>
</div>
@foreach (icart_be.Models.Venda v in Model)
{
    <br>
    <br>
    <p class="texto-ultimas-vendas"> R$@v.Preco_total - @v.Cpf_cliente - @v.Data_compra </p>
}

